{
    "name": "eminoliweb",
    "version": "1.0.0",
    "description": "Web personal de Emiliano Noli",
    "main": "index.js",
    "devDependencies": {
        "@squoosh/lib": "^0.4.0",
        "copy-webpack-plugin": "^11.0.0",
        "css-minimizer-webpack-plugin": "^4.0.0",
        "express": "^4.17.1",
        "html-minimizer-webpack-plugin": "^4.1.0",
        "image-minimizer-webpack-plugin": "^3.3.0",
        "imagemin": "^8.0.1",
        "imagemin-gifsicle": "^7.0.0",
        "imagemin-jpegtran": "^7.0.0",
        "imagemin-optipng": "^8.0.0",
        "imagemin-svgo": "^10.0.1",
        "javascript-obfuscator": "^4.0.0",
        "mini-css-extract-plugin": "^2.6.1",
        "nodemon": "^2.0.20",
        "sharp": "^0.30.7",
        "terser-webpack-plugin": "^5.3.6",
        "webpack": "^5.74.0",
        "webpack-cli": "^4.10.0"
    },
    "scripts": {
        "watch": "nodemon --exec \"npm run build-dev && node nodeserver/express.js\" -e ts,js,html,css --ignore public/",
        "build-dev": "webpack --mode=development",
        "build": "webpack-cli --mode=production",
        "start": "node nodeserver/express.js --mode=production",
        "start-dev": "node src/core/nodeserver/express.js --mode=development",
        "reinstall": "rm -rf node_modules package-lock.json && npm cache clean --force && npm install",
        "git-reset": "git reset --soft HEAD~1"
    },
